{
    "name": "@sencha/ext-web-components-kitchensink",
    "version": "7.2.0",
    "description": "ext-web-components-kitchensink",
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
        "build": "npm run clean && npm run webpackprod && npm run gzip && npm run http",
        "xbuildexample2": "npm run clean && npm run webpackprodexample",
        "buildexample": "cross-env webpack --env.environment=production --env.treeshake=yes --env.basehref=/ExtWebComponents/7.2.0/kitchensink/",
        "buildexamplelocal": "cross-env webpack --env.environment=production --env.treeshake=yes --env.basehref=/",
        "buildexamplegzip": "cross-env webpack --env.environment=production --env.treeshake=yes --env.basehref=/ExtWebComponents/7.2.0/kitchensink/ && npm run gzip",
        "clean": "rimraf build",
        "webpackprod": "cross-env webpack --env.environment=production --env.verbose=no --env.treeshake=yes --env.basehref=/",
        "xwebpackprodexample": "cross-env webpack --env.environment=production --env.treeshake=yes --env.basehref=/ExtWebComponents/7.2.0/kitchensink/",
        "gzip": "npm run gzipbuild && npm run gzipext && npm run gzipresources",
        "gzipbuild": "npx gzip build/*.js",
        "gzipext": "npx gzip build/ext/*.js",
        "gzipresources": "npx gzip build/resources/*.js",
        "replace": "npx replace-in-file /.js/g .js.gz build/index.html --isRegex",
        "http": "npx http-server build -g -o",
        "buildexampleold": "npm run clean && cross-env webpack --env.verbose=no --env.environment=production --env.treeshake=yes --env.basehref=/ExtWebComponents/7.2.0/kitchensink/"
    },
